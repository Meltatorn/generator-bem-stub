{
  "author": "<%= author %>",
  "name": "<%= projectName %>",
  "email": "<%= email %>",
  "version": "0.0.0",
  "ignore": [
    ".enb/tmp",
    "node_modules",
    "libs"
  ],
  "dependencies": {
<%= _.map(libs, function(lib) { return '    "' + lib.name + '": "' + lib.version + '"'}).join(',\n') %>
  },
  "resolutions": {
    <%= '"' + baseLib.name + '": "' + baseLib.version + '"' %>
  }
}
